{
  "project": {
    "version": "<%= version %>",
    "name": "<%= htmlComponentName %>"
  },
  "apps": [
    {
      "root": "dist/aot-tmp/",
      "outDir": "../aot/",
      "assets": "assets",
      "index": "index.html",
      "main": "aot.main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "<%= prefix %>",
      "mobile": <%= isMobile %>,
      "styles": [
        "styles.<%= styleExt %>"
      ],
      "scripts": [],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "<%= styleExt %>",
    "prefixInterfaces": false
  }
}
